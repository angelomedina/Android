<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="estructuras.grafos.estructurasproyect.com.grafos">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".Graficos.Login"
            android:label="@string/title_activity_maps">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Mapas.MapsActivityCargaGrafo"
            android:label="@string/title_activity_maps_carga_grafo" />
        <activity android:name=".Mapas.MainActivity" />
        <activity android:name=".Graficos.Registro" />
        <activity
            android:name=".Graficos.Menu_Usuario"
            android:label="@string/title_activity_menu__usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Mapas.MapsActivityCreaGrafo"
            android:label="@string/title_activity_maps"></activity>
        <activity
            android:name=".Graficos.Menu_Administrador"
            android:label="@string/title_activity_menu__administrador"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>